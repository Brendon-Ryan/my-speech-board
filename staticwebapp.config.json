{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/styles/*", "/scripts/*", "*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".css": "text/css",
    ".js": "text/javascript",
    ".html": "text/html"
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval'; img-src 'self' data: https:; media-src 'self' https:; connect-src 'self' https:"
  },
  "routes": [
    {
      "route": "/",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
}
